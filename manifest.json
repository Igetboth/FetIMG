{
	"manifest_version": 2,
	"name": "FetIMG",
	"version": "0.8",
	"description": "Allows Saveable Images on Fetlife",
	
	"icons": {
		"48": "icons/Icon_48.png",
		"96": "icons/Icon_96.png"
	},
	
	"applications": {
		"gecko": {
			"id": "riley@sunshinesystems.com"
		}
	}, 

	"content_scripts": [
		{
			"matches": ["https://*.fetlife.com/*"],
			"js": ["FetIMG-Min.js"]
		}
	]
}